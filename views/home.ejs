  <%- include('./partials/navbar') %>
  <body class="d-flex text-center text-white bg-dark">
    <div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">
      <header class="mb-auto"></header>
      <main>
        <h1 class="home__title">New Zealand Camps</h1>
        <p class="lead home__subheader">
          Welcome to NZ Camps! <br />
          Jump right in and explore camps across beautiful Aotearoa.
        </p>
        <a
          href="/camps"
          class="btn btn-secondary btn-lg font-weight-bold home__button"
          >View Camps</a
